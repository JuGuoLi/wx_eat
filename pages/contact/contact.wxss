/* pages/contact/contact.wxss */
.whole{
  width:100%;
  height:100%;
  background-color:rgb(22,22,22);
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.whole>view:nth-child(1){
  padding-top:-20rpx;
}
.whole view{
  color:white;
}
.title{
  margin-bottom:100rpx; 
}
.title text{
  font-size:40px;
  font-weight: bold;
}
.whole view:not(:nth-child(1)){
  margin:10rpx 0;
}

